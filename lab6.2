import java.util.*;
public class ListAndSet
{
    public static void main(String[] args)
    {
        Collection<String> list = new ArrayList<String>();
        list.add("Steve");
        list.add("David");
        list.add("Steve");
        System.out.println(list);
        Collection<String> set = new HashSet<String>();
        set.add("Steve");
        set.add("David");
        set.add("Steve");
        System.out.println(set);
    }
} 
